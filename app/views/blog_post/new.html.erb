<h1>Write a new Blog Post</h1>
<%= form_with url:'/blogposts', local:true do |form| %>
<%=    form.label :title  %>
<%=    form.text_field :title %>
<%=    form.label :content %>
<%=   form.text_field :content%>
<%=    form.submit 'Add Blog Post'  %>
    <% end %>
<br>
<%= link_to "Back to Home", blog_posts_path%>